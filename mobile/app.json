{
  "expo": {
    "name": "VOS WASH",
    "slug": "vos-wash",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.voswash.app"
    },
    "android": {
      "package": "com.voswash.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "NOTIFICATIONS",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO",
        "MANAGE_EXTERNAL_STORAGE",
        "CALL_PHONE",
        "RECORD_AUDIO"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "sounds": []
        }
      ],
      "expo-font",
      "expo-asset",
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "Allow VOS WASH to access your microphone to create invoices via voice.",
          "speechRecognitionPermission": "Allow VOS WASH to recognize your speech for invoice commands."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "bf9987c0-4d54-4eea-8b19-126358b1c861"
      }
    }
  }
}