# Server API URL
# For local development
VITE_SERVER_URL=http://localhost:3001

# For testing on local network (replace with your computer's IP)
# VITE_SERVER_URL=http://192.168.1.100:3001

# For production deployment
# VITE_SERVER_URL=https://your-server-domain.com

# API Key for secured endpoints (must match server API_KEY)
VITE_API_KEY=your_secret_api_key_here

# MongoDB Connection (only needed if running local DB)
VITE_MONGODB_CONNECTION_STRING=mongodb+srv://username:password@cluster.mongodb.net/vosWashDB?retryWrites=true&w=majority
